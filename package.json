    "@types/supertest": "^2.0.8",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^4.0.2",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
